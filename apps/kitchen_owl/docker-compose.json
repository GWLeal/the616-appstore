{
    "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
    "services": [
        {
            "name": "front",
            "image": "tombursch/kitchenowl-web:latest",
            "isMain": true,
            "internalPort": 80,
            "dependsOn": ["back"]
        },
        {
            "name": "back",
            "image": "tombursch/kitchenowl-backend:latest",
            "environment": {
                "JWT_SECRET_KEY": "${JWT_SECRET_KEY}"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/data"
                }
            ]
        }
    ]
}
